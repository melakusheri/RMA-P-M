<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">RVA Park and Museum</h1>
    <p class="lead">Richmond Metro Area Parks and Museums Guide</p>
  </div>
</div>
<div class="container-fluid d-flex flex-wrap justify-content-between w-100">
    {{#each Counties as |county|}}
    <div class="container-fluid col-md-4 col-sm-12">
        <a href="/county/{{county.id}}">
        <div class="card bg-dark text-white">
            <img class="card-img img-fluid home-card-img" src="{{county.picture}}" alt="Card image">
                <div class="card-img-overlay">
                    <h5 class="card-title">{{county.name}}</h5>
                </div>
        </div>
        </a>
    </div>
    {{/each}}
</div>